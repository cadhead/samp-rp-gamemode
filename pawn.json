{
  "user": "cadhead",
	"repo": "samp-rp-gamemode",
	"entry": "src/main.pwn",
	"output": "gamemodes/samp-rp-gamemode.amx",
  "dependencies": [
    "pawn-lang/pawn-stdlib",
    "pBlueG/SA-MP-MySQL",
    "Y-Less/sscanf",
    "samp-incognito/samp-streamer-plugin",
    "Southclaws/pawn-chrono",
    "pawn-lang/sa-mp-fixes",
    "MySekwel/New-SA-MP-callbacks",
    "pawn-lang/YSI-Includes@5.x",
    "kristoisberg/samp-map-zones",
    "katursis/Pawn.CMD:3.3.4",
    "Open-GTO/mdialog"
  ],
  "local": true,
  "runtime": {
    "version": "0.3.7",
    "mode": "server",
    "gamemodes": [
      "samp-rp-gamemode"
    ],
    "rcon_password": "password",
    "port": 7777,
    "hostname": "RolePlay Gamemode (0.3.7)",
    "maxplayers": 150,
    "language": "Russian",
    "mapname": "Los Santos",
    "weburl": "www.sa-mp.com",
    "gamemodetext": "samp-rp-gamemode",
    "announce": true,
    "lanmode": false,
    "query": true,
    "rcon": false,
    "logqueries": false,
    "sleep": 5,
    "maxnpc": 0,
    "stream_rate": 1000,
    "stream_distance": 200,
    "onfoot_rate": 30,
    "incar_rate": 30,
    "weapon_rate": 30,
    "chatlogging": false,
    "timestamp": true,
    "messageholelimit": 3000,
    "messageslimit": 500,
    "ackslimit": 3000,
    "playertimeout": 10000,
    "minconnectiontime": 0,
    "lagcompmode": 1,
    "connseedtime": 300000,
    "db_logging": false,
    "db_log_queries": false,
    "conncookies": true,
    "cookielogging": false,
    "output": true
  },
  "builds": [{
    "name": "dev",
    "args": [
    	"-;+",
    	"-(+",
    	"-Z+",
    	"-S32768",
    	"-d3"
    ],
    "includes": [
      "src/modules",
      "src/modules/Preincludes",
      "src/modules/Admin",
      "src/modules/Arrays",
      "src/modules/Character",
      "src/modules/Player",
      "src/modules/Server",
      "src/modules/Systems"
    ]
  }]
}
